{
  "private": true,
  "name": "notebook-vue",
  "version": "1.0.0",
  "scripts": {
    "start": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "commit": "git add . && git cz && git push",
    "test": "vitest",
    "test:watch": "vitest -w",
    "docs:deploy": "gh-pages -d docs-dist",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\""
  },
  "main": "dist/index.js",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "typescript": "^4.5.4",
    "vitepress": "1.0.0-alpha.4",
    "vitest": "^0.14.0",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "naive-ui": "^2.16.2",
    "vue": "^3.2.9"
  }
}
